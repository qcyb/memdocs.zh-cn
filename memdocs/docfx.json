{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "exclude": [
          "**/obj/**",
          "intune-azure/**",
          "**/includes/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/media/**",
          "**/*.png",
          "**/*.gif",
          "**/*.jpg"
        ],
        "exclude": [
          "**/obj/**",
          "intune-azure/**",
          "**/includes/**"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "breadcrumb_path": "/mem/breadcrumb/toc.json",
      "brand": "Intune",
      "searchScope": ["Intune"]
    },
    "fileMetadata": {
      "featureFlags": {
        "*.md" : [
          "show_binary_rating"
        ]
      }
    },
    "template": [],
    "dest": "intune-azure",
    "markdownEngineName": "markdig"
  }
}
